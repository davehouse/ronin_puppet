  "tasksDir": "<%= @task_dir %>",
  "workerGroup": "<%= @worker_group %>",
  "workerId": "<%= @hostname %>",
  "workerType": "<%= @worker_type %>",
  "workerTypeMetadata": {
    "machine-setup": {
      "config": "https://github.com/mozilla-platform-ops/ronin_puppet/raw/master/modules/generic_worker/templates/generic-worker.config.erb",
      "docs": "https://wiki.mozilla.org/ReleaseEngineering/PuppetAgain/Modules/generic_worker"
